{
  "name": "kernel-packet-tracer-frontend",
  "version": "2.0.0",
  "description": "Frontend for Kernel Packet Tracer with Realtime Visualization",
  "private": true,
  "dependencies": {
    "axios": "^1.13.2",
    "plotly.js": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f ../.env.development react-scripts start",
    "start:test": "env-cmd -f ../.env.testing react-scripts start",
    "start:staging": "env-cmd -f ../.env.staging react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f ../.env.development react-scripts build",
    "build:test": "env-cmd -f ../.env.testing react-scripts build",
    "build:staging": "env-cmd -f ../.env.staging react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test --watchAll=false --coverage",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}
